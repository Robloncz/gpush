    "@aws-sdk/client-bedrock-runtime": "^3.734.0",
    "@aws-sdk/credential-providers": "^3.734.0",
    "@clack/prompts": "^0.9.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "openai": "^4.0.0",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
